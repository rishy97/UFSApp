      <Button fx:id="copyAllButton" alignment="CENTER" layoutX="663.0" layoutY="655.0" mnemonicParsing="false" onAction="#copyAllEmails" prefHeight="53.0" prefWidth="212.0" text="Copy All" textAlignment="CENTER">
